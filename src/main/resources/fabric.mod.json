{
  "schemaVersion": 1,
  "id": "npe",
  "version": "${version}",

  "name": "No Prying Eyes",
  "description": "A simple Minecraft mod that aims to give the user control over the collection of their data and attempts to free the game of Microsofts control.",
  "authors": [
    "Daxanius"
  ],
  "contact": {
    "homepage": "https://github.com/Daxanius",
    "sources": "https://github.com/Daxanius/NoPryingEyes"
  },

  "license": "CC0-1.0",
  "icon": "assets/npe/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.npe.NoPryingEyes"
    ]
  },
  "mixins": [
    "npe.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
